<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/venues_graph"
    app:startDestination="@id/venues_destination">

    <fragment
        android:id="@+id/venues_destination"
        android:name="com.bookmyshow.venues.ui.VenuesFragment"
        tools:layout="@layout/fragment_venues" >
        <action
            android:id="@+id/action_venues_destination_to_show_time_info_destination"
            app:destination="@id/show_time_info_destination" />
    </fragment>

    <fragment
        android:id="@+id/show_time_info_destination"
        tools:layout="@layout/fragment_show_time_info"
        android:name="com.bookmyshow.seats.ui.ShowTimeInfoFragment"
        android:label="How many Seats?" />


</navigation>